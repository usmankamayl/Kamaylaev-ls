.skill {
  position: relative;
  width: 166px;
  height: 166px;
  &__circle {
    fill: transparent;
    &_under {
      stroke: #3B3A3A;
      stroke-width: 3px;
    }
    
    &_above {
      stroke-width: 5px;
      stroke-dasharray: 251;
      stroke-dashoffset: 100;
    }
  }

  

  &__circle-text {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
  }
}